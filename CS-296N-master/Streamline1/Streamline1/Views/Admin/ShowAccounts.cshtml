@model IEnumerable<User>
@{
    ViewData["Title"] = "Show All Accounts";
}

<h4>User Accounts</h4>
<table>
    <tr>
        <th>ID</th>
        <th>Username</th>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr><td colspan="3">No User Accounts</td></tr>
    }
    else
    {
        foreach (User user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>              
            </tr>
        }
    }
</table>
<a asp-action="CreateAccount">Create an Account</a><br />
<a asp-action="Index">Admin Page</a>